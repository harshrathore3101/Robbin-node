    </section>
    <!-- Related items section-->
    <section id="Relatedproducts" class="py-5 bg-light">
      <div class="container px-4 px-lg-5 mt-5">
        <h2 class="fw-bolder mb-4">Related products</h2>
        <div
          class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-4 justify-content-center"
        >
          <% data.forEach(function(val){ %> <% if( val.type === prd.type){%>
          <div class="col mb-5">
            <div class="card h-100">
              <a class="black" href="/loginfo/<%= val._id %>">
                <!-- Product image-->
                <img
                  class="card-img-top"
                  src="../images/<%= val.img2 %>"
                  alt="..."
                />
                <!-- Product details -->
                <div class="card-body p-4">
                  <!-- <a id="info"> aj</a> -->
                  <div class="text-center">
                    <!-- Product name -->
                    <h5 class="fw-bolder"><%= val.name %></h5>
                    <!-- Product price-->
                    Rs <%= val.price %>/-
                  </div>
                </div>
                <!-- Product actions-->
                <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">
                  <div class="text-center">
                     <% if(user.product.indexOf( val._id ) === -1 ){%>
                      <a
                        href="/card/<%= val._id %>"
                        class="btn btn-outline-dark flex-shrink-0 ms-3"
                      >
                        <i class="bi-cart-fill me-1"></i>
                        Add to cart
                      </a>
                      <% } else{ %>
                      <a
                        href="/card/<%= val._id %>"
                        class="btn btn-outlines-dark flex-shrink-0 ms-3"
                      >
                        <i class="bi-cart-fill me-1"></i>
                        Added
                      </a>
                      <% } %>
                      <!-- <p class="value"><%= val._id %></p> -->
                    </div>
                </div>
              </a>
            </div>
          </div>
          <% }%> <% }) %>
        </div>
      </div>
      <hr id="YoumayalsoLike">
      <!-- ////////////////////////////////////////// -->
      <div class="container px-4 px-lg-5 mt-5">
        <h2 class="fw-bolder mb-4">You may also Like</h2>
        <div
