      <section id="allprd" class="py-5">
        <div class="container px-4 px-lg-5 mt-5">
          <h1>Hot selling</h1>
          <br />
          <div
            class="row gx-4 gx-lg-5 row-cols-2 row-cols-md-3 row-cols-xl-4 justify-content-center"
          >
            <% data.forEach(function(val){ %> <% if( val.hotsell==="yes"){%>
            <div class="col mb-5">
              <div class="card h-100">
                <div class="sell">
                  <p>Hot sell</p>
                </div>
                <a class="black" href="/info/<%= val._id %>">
                  <!-- Product image-->
                  <img
                    class="card-img-top"
                    src="../images/<%= val.img1 %>"
                    alt="..."
                  />
                  <!-- Product details -->
                  <div class="card-body p-4">
                    <!-- <a id="info"> aj</a> -->
                    <div class="text-center">
                      <!-- Product name -->
                      <h5 class="fw-bolder"><%= val.name %></h5>
                      <!-- Product price-->
                      Rs <%= val.price %>/-
                    </div>
                  </div>
                  <!-- Product actions-->
                  <div class="card-footer p-4 pt-0 border-top-0 bg-transparent">
                    <div class="text-center">
                      <a
                        class="btn btn-outline-dark mt-auto"
                        href="/card/<%= val._id %>"
                      >
                        Add to Cart</a
